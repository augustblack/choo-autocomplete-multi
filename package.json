  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "bank": "./node_modules/.bin/bankai autocomplete.js --open ",
    "watch": "./node_modules/.bin/budo autocomplete.js --live --open --pushstate",
    "start": "budo index.js:static/bundle.js --pushstate --live --open -- -t sheetify/transform -t envify -t babelify",
    "startbankai": "bankai start index --open -t sheetify/transform -t envify -t babelify -t yo-yoify",
    "build": "rm -rf dist && mkdir -p dist && NODE_ENV=production browserify -e index.js -t sheetify/transform  -t babelify -g unassertify > dist/bundle.js",
    "buildit": "rm -rf dist && mkdir -p dist && NODE_ENV=production browserify -e index.js -t envify -t yo-yoify -t babelify  -g unassertify -g sheetify -g uglifyify  | uglifyjs > dist/bundle.js"
  },
